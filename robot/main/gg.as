			$cnt[0] = $ENCODE (/D, numbytes)
			UDP_SENDTO ret, ip[1], port, $cnt[0], 1, timeout
			IF ret <> 0 THEN
				PRINT "Error with the UDP send, code: ", ret
			ELSE
				; Recieve confirmation code or error code
				UDP_RECVFROM ret, port, $cnt[0], numbytes, timeout, ip[1]
