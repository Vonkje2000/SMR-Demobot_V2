.PROGRAM main1.pc() ;
	SIGNAL -2254, -2245
	sock_id1 = -1
	.port = 8700
	.$receive = ""
	.retrecv = -1
	WAIT SWITCH (POWER )
connection:
	CALL tcp_open1 (.port)
	;PRINT sock_id1
	IF sock_id1 < 0 GOTO connection
receive:
	.$receive = ""
	CALL tcp_recv1 (.retrecv, .$receive)
	IF .retrecv < 0 GOTO receive
	IF .$receive == "start" THEN
		MC EXECUTE test4
		GOTO receive
	END
	IF .$receive == "stop" THEN
		CALL tcp_close2
		MC ABORT test4
		GOTO connection
		;PRINT sock_id1
	END
.END
