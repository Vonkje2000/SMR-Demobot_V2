				PRINT "TCP_CLOSE error id = ", sock_id1
			ELSE
				PRINT "TCP_CLOSE OK id = ", sock_id1
			END
			GOTO connect
		END
	END
	IF sock_id1 != 0 THEN
		ret = 0
		TCP_CLOSE ret, 0
		GOTO connect
	END
rpss:
	; Define the actions to perform when the connection is successful
	; This function will be called whenever the connection is established successfully
.END
