	;
	.$temp = $DECODE (.$full_message, "|", 1)
	.$joint0 = $DECODE (.$full_message, "|", 0)
	.joint0 = VAL (.$joint0)
	;
	.$temp = $DECODE (.$full_message, "|", 1)
